<% UserSettings.where(user_id: current_user.id).each do |u| %>
  <% if u.plugin_id == 0 %>
    <%= link_to "AoD Time Entry", plugin_aod_time_entry_index_path, 
        class: "js-plugin" %>
  <% elsif u.plugin_id == 1 %>
    <%= link_to "Protocall Visualizer", plugin_visualizer_index_path, 
        class: "js-plugin" %>
  <% elsif u.plugin_id == 2 %>
    <%= link_to "FMC Payroll Export", plugin_fmc_index_path, 
        class: "js-plugin" %>
  <%# elsif u.plugin_id == 3 %>
    <%#= link_to "Snohomish Timecard Editor", plugin_snohomish_index_path, 
        class: "js-plugin" %>
  <% elsif u.plugin_id == 4 %>
    <%= link_to "Service Master", plugin_service_master_index_path, 
        class: "js-plugin" %>
  <% elsif u.plugin_id == 5 %>
    <%= link_to "BambooHR", plugin_bamboo_hr_index_path, 
        class: "js-plugin" %>
  <% end %>
<% end %>

<!-- Scripts -->
<script>
  // Show the spinner when you click on a plugin
    $('.js-plugin').click(function() {
    $('#spinner').show();
  });
</script>